{
  "AccessTokenExpireTimeSpan": "30",
  "AuthorizationRoutePath": "api/user/Authentication",
  "AuthorizationServiceKey": "User",
  "TokenEndpointPath": "api/user/LoginByPhoneAsync",
  "CacheMode": "MemoryCache", //MemoryCache or  gateway.Redis save token   ddlCache.Redis
  //跨域配置
  "AccessPolicy": {
    "Origins": [ "http://127.0.0.1:1927", "http://localhost:927" ],
    "AllowAnyHeader": true,
    "AllowAnyMethod": true
  },
  //注册中心配置（zookeeper或者consul）
  "Register": {
    "Provider": "Consul",
    "Address": "${Register_Conn}|127.0.0.1:8500"
  },
  //服务聚合配置
  "ServicePart": {
    "MainPath": "part/service/aggregation",
    "EnableAuthorization": false,
    "Services": [
      {
        "ServiceAggregation": [
          {
            "RoutePath": "api/user/GetUserName",
            "ServiceKey": "User",
            "Key": "Users"
          },
          {
            "RoutePath": "api/user/GetUserName",
            "ServiceKey": "User",
            "Key": "Roles"
          }
        ],
        "UrlMapping": "part/user/getuserrole"
      }
    ]
  }
}