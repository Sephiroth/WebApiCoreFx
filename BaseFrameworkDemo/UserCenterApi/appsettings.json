{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "ConsulAddress": "http://192.168.233.128:8500", //Consul服务注册地址，如果是消费者，则只需要配置这个字段，其它的无需配置
    "ServiceName": "UserCenterApi", //当前服务名称，可以多个实例共享
